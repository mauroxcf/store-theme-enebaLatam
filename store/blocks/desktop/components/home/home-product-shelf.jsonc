{
  "responsive-layout.desktop#home__slider--children--four": {
    "children": [
      "list-context.product-list#home__slider--children--four-desktop"
    ]
  },
  "list-context.product-list#home__slider--children--four-desktop": {
    "blocks": ["product-summary.shelf#home__gift_card-desktop"],
    "children": ["slider-layout#demo-products"]
  },
  "product-summary.shelf#home__gift_card-desktop": {
    "children": ["flex-layout.col#product__container-desktop"]
  },
  "flex-layout.col#product__container-desktop": {
    "children": [
      "flex-layout.col#product__image",
      "flex-layout.col#product__information"
    ],
    "props": {
      "blockClass": "product__container"
    }
  },
  "flex-layout.col#product__image": {
    "children": ["product-summary-image#shelf"]
  },
  "product-summary-image#shelf": {
    "props": {
      "showBadge": false,
      "aspectRatio": "1:1"
    }
  },
  "flex-layout.col#product__information": {
    "children": [
      "product-summary-name",
      "rich-text#product__summary-region",
      "rich-text#product__summary-at",
      "product-selling-price"
    ]
  },
  "rich-text#product__summary-region": {
    "props": {
      "text": "GLOBAL",
      "blockClass": "product__summary-region"
    }
  },
  "rich-text#product__summary-at": {
    "props": {
      "text": "Desde",
      "blockClass": "product__summary-at"
    }
  }
}
