    "children": ["flex-layout.col#trustpilot__container"]
  },
  "flex-layout.col#trustpilot__container": {
    "children": ["rich-text#trustpilot__title", "logo#social__trustpilot"],
    "props": {
      "blockClass": "footer__option-container"
    }
  },
  "rich-text#trustpilot__title": {
    "props": {
      "text": "Ver nuestros comentarios en",
      "blockClass": "footer__option-title"
    }
  },
  "logo#social__trustpilot": {
    "props": {
      "title": "trustpilot",
      "url": "assets/svg/social_networks/trustpilot.svg",
      "height": 34,
      "blockClass": "social-icon"
    }
  }
}
