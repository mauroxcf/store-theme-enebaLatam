{
  "flex-layout.col#product__image": {
    "title": "product image content",
    "children": ["product-summary-image#shelf"]
  },
  "product-summary-image#shelf": {
    "title": "product image summary",
    "props": {
      "showBadge": false,
      "aspectRatio": "1:1"
    }
  },
  "flex-layout.col#product__information": {
    "title": "product information content",
    "children": [
      "product-summary-name",
      "rich-text#product__summary-region",
      "rich-text#product__summary-at",
      "product-selling-price",
      "add-to-cart-button"
    ],
    "props": {
      "blockClass": "product__information"
    }
  },
  "flex-layout.col#product__information-no_add_button": {
    "title": "product information content no add button",
    "children": [
      "product-summary-name",
      "rich-text#product__summary-region",
      "rich-text#product__summary-at",
      "product-selling-price"
    ],
    "props": {
      "blockClass": "product__information-no_add_button"
    }
  },
  "rich-text#product__summary-region": {
    "title": "region text",
    "props": {
      "text": "GLOBAL",
      "blockClass": "product__summary-region"
    }
  },
  "rich-text#product__summary-at": {
    "title": "at text",
    "props": {
      "text": "Desde",
      "blockClass": "product__summary-at"
    }
  }
}
